//code for make the transpose of matrix
#include <stdio.h>

int main()
{
    int m,n,i,j;
    printf("Enter the number of rows and columns : ");
    scanf("%d %d",&m,&n);
    printf("Enter the elements\n");
    int arr[m][n];
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        {
            scanf("%d",&arr[i][j]);
            
        }
    }
    if(m==n)
    {
        for(i=0;i<m;i++)
        {
            for(j=0;j<n;j++)
            {
                printf("%d ",arr[j][i]);
            }
            printf("\n");
        }
    }
    else 
    {
        printf("transpose is not possible");
    }
    return 0;
}
